        <!--定时器配置-->
        <property name="quartz.run" defaultValue="false" description="定时器"/>
        <!--主redis配置-->
        <property name="birth.master.jedis.host" defaultValue="121.199.28.244" description="写redisIP"/>
        <property name="birth.master.jedis.port" defaultValue="6379" description="redis端口"/>
        <property name="birth.master.jedis.pwd" defaultValue="xZ7ScgiMV1Acv3h4" description="redis密码"/>
